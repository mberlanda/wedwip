/ Instructions: Set your background image using the URL below.
%header{ style: "background-image: url(#{asset_path('img/fashion/stockholm.jpg')})" }
  %aside.header-quote
    .container.wow.fadeIn
      .row
        .col-md-10.col-md-offset-1
          / %span.quote= t('header.date')
          %span.quote
            %span.text-primary= t('header.text-primary')
          %hr.colored
          %span.quote= t('header.quote')
          %br
          .col-md-12
            %a.btn.btn-outline-light.page-scroll{ href: "#register" }= t('header.buttons.signup')

  .scroll-down
    %a.btn.page-scroll{ href: "#register" }
      %i.fa.fa-angle-down.fa-fw